<div>
    <h1>タスク一覧</h1>
    <table>
        <tr>
            <th>id</th>
            <th>タスクの内容</th>
            <th>ステータス</th>
        </tr>
        
        
        <% @current_user.tasks.each do |task| %>
            <tr>
                <td><%= link_to task.id, task_path(task) %></td>
                <td><%= task.content %></td>
                <td><%= task.status %></td>
            </tr>
        <% end %>
    </table>
    <%= link_to "新規作成", new_task_path %>
    <%= link_to "Logout", logout_path, method: :delete %>
</div>